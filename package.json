{
  "name": "@tosvg/node",
  "version": "1.0.0",
  "description": "Official Node.js SDK for the ToSVG API â€” Image to SVG conversion, background removal, and image resizing",
  "author": "ToSVG",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "tosvg",
    "svg",
    "image-to-svg",
    "vector",
    "background-removal",
    "image-resize",
    "raster-to-vector",
    "api",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tosvg/tosvg-node"
  },
  "homepage": "https://tosvg.com",
  "bugs": {
    "url": "https://github.com/tosvg/tosvg-node/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "msw": "^2.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0"
  }
}
